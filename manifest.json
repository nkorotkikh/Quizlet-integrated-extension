{
  "name": "Extension for integration with Quizlet",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "storage",
    "notifications",
    "alarms",
    "identity",
    "https://quizlet.com/*",
    "https://api.quizlet.com/*"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}